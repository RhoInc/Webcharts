{
  "name": "webcharts",
  "description": "A library for creating flexible, interactive charts",
  "version": "1.7.1",
  "keywords": [
    "charts",
    "javascript",
    "interactive",
    "data visualization"
  ],
  "homepage": "https://github.com/RhoInc/Webcharts",
  "main": "lib/webcharts.common.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "d3": "~3"
  },
  "scripts": {
    "uglifyhtml":"uglifyjs build/webcharts.js > build/webcharts.min.js",
    "uglifycss":"uglifycss css/webcharts.css > css/webcharts.min.css",
    "uglify":"npm run uglifyhtml && npm run uglifycss",
    "rollup":"rollup -c ./scripts/rollup.config.js | babel > ./build/webcharts.js",
    "build":"npm run rollup && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhoinc/webcharts.git"
  },
  "author": "Rho, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhoinc/webcharts/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "rollup": "^0.25.1",
    "rollup-plugin-babel": "^2.4.0",
    "uglify-js": "^2.5.0",
    "uglifycss":"~0"
  }
}
