{
  "name": "webcharts",
  "description": "A library for creating flexible, interactive charts",
  "version": "2.0.0-rc.1",
  "keywords": [
    "charts",
    "javascript",
    "interactive",
    "data visualization"
  ],
  "homepage": "https://github.com/RhoInc/Webcharts",
  "main": "webcharts.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "d3": "~3.5"
  },
  "scripts": {
    "build": "rollup -c > ./webcharts.js",
    "minify": "uglifyjs ./webcharts.js > ./webcharts.min.js",
    "start": "npm run build && npm run minify",
    "test": "mocha --compilers js:babel-register --recursive ./test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhoinc/webcharts.git"
  },
  "author": "Rho, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhoinc/webcharts/issues"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.7.2",
    "expect": "^1.18.0",
    "jsdom": "^8.4.1",
    "mocha": "^2.4.5",
    "rollup": "^0.26.0",
    "rollup-plugin-babel": "^2.4.0",
    "uglify-js": "^2.5.0"
  }
}
